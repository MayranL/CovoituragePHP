                            <p class="flex text-gray-600 mb-4">
                                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6">
                                    <path stroke-linecap="round" stroke-linejoin="round" d="M12 6v6h4.5m4.5 0a9 9 0 11-18 0 9 9 0 0118 0z" />
                                </svg>

                                Date : {{ reservation.annonce.date|date('d/m/Y à H:i') }}</p>
                            <div class="flex justify-end">
                                <a href="{{ path('afficher_annonce', {'id': reservation.annonce.id}) }}" class="block mt-4 text-indigo-500 hover:underline">
                                    <svg class="w-5 h-5 ml-2 -mr-1" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
                                        <path fill-rule="evenodd" d="M10.293 3.293a1 1 0 011.414 0l6 6a1 1 0 010 1.414l-6 6a1 1 0 01-1.414-1.414L14.586 11H3a1 1 0 110-2h11.586l-4.293-4.293a1 1 0 010-1.414z" clip-rule="evenodd"></path>
                                    </svg>
                                </a>
                            </div>
                        </div>
                    {% endif %}
                {% endfor %}

            </div>
        </div>

        <div class="mb-8">
        <h2 class="text-2xl font-bold mb-4">Trajets effectuées</h2>
        <div class="grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-4 gap-4">
            {% for annonce in annonces %}
                <div class="bg-white rounded-lg shadow-lg p-4">
                    <!-- Contenu de la réservation -->
                    <h2 class="flex text-xl font-bold mb-2">
                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6">
                            <path stroke-linecap="round" stroke-linejoin="round" d="M15 10.5a3 3 0 11-6 0 3 3 0 016 0z" />
